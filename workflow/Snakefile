rule all:
    input:
        expand(
            "results/dataset/{dataset}/features/{features}.parquet",
            dataset=config["datasets"],
            features=[
                "Enformer_L2",
                "Borzoi_L2",
                "CADD",
                "GPN-MSA_LLR",
                "GPN-MSA_InnerProducts",
                "GPN-MSA_Embeddings",

                #"NucleotideTransformer_LLR",
                #"NucleotideTransformer_InnerProducts",

                "GPN_final_LLR",
                "GPN_final_Embeddings",
                "GPN_final_InnerProducts",

                #"HyenaDNA_LLR",
